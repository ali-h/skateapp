    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@fontsource/plus-jakarta-sans": "^4.5.11",
    "@hiveio/dhive": "^1.2.7",
    "@pioneer-platform/pioneer-client": "^9.0.6",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "events": "^3.3.0",
    "framer-motion": "^10.16.0",
    "keychain-sdk": "^0.4.0",
    "markdown-it": "^13.0.1",
